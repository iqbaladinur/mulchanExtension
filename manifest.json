{
    "name": "Page Redder",
    "action": {},
    "manifest_version": 3,
    "version": "0.1",
    "description": "Turns the page red when you click the icon",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": ["https://multichannel.qiscus.com/web/*"],
          "js": ["styleExtension.js"]
        }
    ]
}